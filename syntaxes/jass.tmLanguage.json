{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Jass",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#storages"
		},
		{
			"include": "#constants"
		},
		{
			"include": "#numbers"
		},
		{
			"include": "#opt"
		},
		{
			"include": "#comments"
		},
		{
			"include": "#functionsa"
		}
	],
	"repository": {
		"comments": {
			"patterns": [
				{
					"captures": {
						"1": {
							"name": "meta.toc-list.banner.block.jass"
						}
					},
					"match": "^/\\* =(\\s*.*?)\\s*= \\*/$\\n?",
					"name": "comment.block.jass"
				},
				{
					"begin": "/\\*",
					"beginCaptures": {
						"0": {
							"name": "punctuation.definition.comment.begin.jass"
						}
					},
					"end": "\\*/",
					"endCaptures": {
						"0": {
							"name": "punctuation.definition.comment.end.jass"
						}
					},
					"name": "comment.block.jass"
				},
				{
					"match": "\\*/.*\\n",
					"name": "invalid.illegal.stray-comment-end.jass"
				},
				{
					"captures": {
						"1": {
							"name": "meta.toc-list.banner.line.jass"
						}
					},
					"match": "^// =(\\s*.*?)\\s*=\\s*$\\n?",
					"name": "comment.line.banner.jass"
				},
				{
					"begin": "(^[ \\t]+)?(?=//)",
					"beginCaptures": {
						"1": {
							"name": "punctuation.whitespace.comment.leading.cpp"
						}
					},
					"end": "(?!\\G)",
					"patterns": [
						{
							"begin": "//",
							"beginCaptures": {
								"0": {
									"name": "punctuation.definition.comment.cpp"
								}
							},
							"end": "(?=\\n)",
							"name": "comment.line.double-slash.cpp",
							"patterns": [
								{
									"match": "(\\\\)\\n",
									"captures": {
										"1": {
											"name": "constant.character.escape.line-continuation.c"
										}
									}
								}
							]
						}
					]
				}
			]
		},
		"keywords": {
			"patterns": [{
				"name": "keyword.control.jass",
				"match": "\\b(globals|endglobals|function|endfunction|takes|returns|return|library|endlibrary|local|set|call|public|private|initializer|uses|requires|needs|if|then|else|endif|loop|exitwhen|endloop|type|extends|native)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.jass",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.jass",
					"match": "\\\\."
				}
			]
		},
		"storages":{
			"patterns": [
				{
					"name": "storage.type.jass",
					"match": "\\b(agent|integer|real|string|boolean|handle|code|ability|aidifficulty|alliancetype|blendmode|boolexpr|buff|button|camerafield|camerasetup|conditionfunc|defeatcondition|destructable|dialog|dialogevent|effect|effecttype|event|eventid|fgamestate|filterfunc|fogmodifier|fogstate|force|gamecache|gamedifficulty|gameevent|gamespeed|gamestate|gametype|group|igamestate|item|itempool|itemtype|leaderboard|limitop|location|mapcontrol|mapdensity|mapflag|mapsetting|mapvisibility|multiboard|multiboarditem|placement|player|playercolor|playerevent|playergameresult|playerscore|playerslotstate|playerstate|playerunitevent|quest|questitem|race|racepreference|raritycontrol|rect|region|sound|startlocprio|terraindeformation|texmapflags|texttag|timer|timerdialog|trackable|trigger|triggeraction|triggercondition|unit|unitevent|unitpool|unitstate|unittype|version|volumegroup|weathereffect|widget|widgetevent|hashtable|attacktype|damagetype|weapontype|soundtype|lightning|pathingtype|image|ubersplat|array)\\b"
				}
			]
		},
